{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
            "type": "python",
            "style": "plain",
            "foreground": "#eb6f92",
            "template": "îˆµ {{ .Full}} {{ .Venv }} "
        },
        {
          "foreground": "#eb6f92",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        },
        {
          "foreground": "#ebbcba",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#c4a7e7",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": "<#31748f>git:(</>{{ .HEAD }}<#31748f>)</>",
          "type": "git"
        },
        {
          "foreground": "#BF616A",
          "style": "plain",
          "template": " \u2717",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
